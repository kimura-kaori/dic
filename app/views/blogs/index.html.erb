<h1>投稿一覧</h1>
  <table>
    <% @blogs.each do |blog| %>
    <th>投稿者：</th>
    <tr>
      <td><%= blog.title %></td>
    </tr>
    <tr>
      <td><%= blog.content %></td>
    </tr>
    <tr>
      <td>
        <%= link_to 'Show', picture %>
        /<%= link_to 'Edit', edit_picture_path(picture) %>
        /<%= link_to 'Destroy', picture, method: :delete, data: { confirm: '本当に消しますか？' } %>
        <% end %>
      </td>
    </tr>
    <% end %>
  </table>
